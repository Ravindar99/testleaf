package week2.day1;

import org.openqa.selenium.By;
import org.openqa.selenium.chrome.ChromeDriver;

public class LoadUrl {
public static void main(String[] args) throws InterruptedException {
	ChromeDriver fb=new ChromeDriver();
	fb.get("https://www.facebook.com/");
	fb.manage().window().maximize();
	fb.findElement(By.xpath("//div[@class='inputtext _55r1 _6luy](1)")).sendKeys("testleaf.2023@gmail.com");
	fb.findElement(By.xpath("//div[@class='inputtext _55r1 _6luy](2)")).sendKeys("Tuna@321");
	fb.findElement(By.xpath("//div[@class='_42ft _4jy0 _6lth _4jy6 _4jy1 selected _51sy']")).click();
	System.out.println(fb.getTitle());

	
}
}
